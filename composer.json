{
    "name": "swisnl/guzzle-bugsnag-breadcrumbs-middleware",
    "type": "library",
    "description": "Guzzle middleware that logs Bugsnag breadcrumbs",
    "keywords": [
        "swisnl",
        "guzzle",
        "middleware",
        "bugsnag",
        "breadcrumbs"
    ],
    "homepage": "https://github.com/swisnl/guzzle-bugsnag-breadcrumbs-middleware",
    "license": "MIT",
    "authors": [
        {
            "name": "Jasper Zonneveld",
            "email": "jasper@swis.nl",
            "homepage": "https://github.com/JaZo",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4",
        "bugsnag/bugsnag": "^3.20",
        "guzzlehttp/guzzle": "^7.2"
    },
    "require-dev": {
        "bugsnag/bugsnag-laravel": "^2.21",
        "friendsofphp/php-cs-fixer": "^3.5",
        "phpunit/phpunit": ">=9.5"
    },
    "autoload": {
        "psr-4": {
            "Swis\\Guzzle\\Bugsnag\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Swis\\Guzzle\\Bugsnag\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "php-cs-fixer fix --dry-run -v",
        "fix-style": "php-cs-fixer fix"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
